{
  "name": "cleepix",
  "version": "1.0.0",
  "description": "It is an integrated management application for bookmarks, canned messages, clipboard, and RSS.",
  "main": "./compiled/main/index.js",
  "author": "example.com",
  "homepage": "https://www.electronjs.org",
  "types": "index.d.ts",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview -c electron.vite.config.dev.ts",
    "dev": "electron-vite dev -c electron.vite.config.dev.ts",
    "build": "npm run typecheck && electron-vite build -c electron.vite.config.prod.ts",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.3",
    "@types/kuromoji": "^0.1.1",
    "axios": "^1.4.0",
    "better-sqlite3-multiple-ciphers": "^8.1.0",
    "bookmark-file-parser": "^1.0.6",
    "cheerio": "^1.0.0-rc.12",
    "clipboard-event": "^1.6.0",
    "electron-store": "^8.1.0",
    "file-type": "^18.2.1",
    "file-type-cjs-fix": "^1.1.1",
    "image-type": "^5.2.0",
    "kuromoji": "^0.1.2",
    "puppeteer-core": "^19.11.1",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron-toolkit/utils": "^1.0.2",
    "@electron/notarize": "^1.2.3",
    "@fortawesome/fontawesome-free": "^6.3.0",
    "@types/better-sqlite3": "^7.6.3",
    "@types/node": "18.15.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "animate.css": "^4.1.1",
    "electron": "^23.1.3",
    "electron-builder": "^23.6.0",
    "electron-rebuild": "^3.2.9",
    "electron-vite": "^1.0.14",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.4",
    "quill": "^1.3.7",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-sass-dts": "^1.3.1"
  }
}
